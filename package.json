{
    "name": "@nodecfdi/sat-ws-descarga-masiva",
    "version": "0.1.2",
    "description": "Librer√≠a para usar el servicio web del SAT de Descarga Masiva",
    "keywords": [
        "sat",
        "cfdi",
        "download",
        "descarga",
        "webservice"
    ],
    "private": false,
    "author": "OcelotlStudio <contacto@ocelotlstudio.com>",
    "license": "MIT",
    "type": "commonjs",
    "main": "./dist/sat-ws-descarga-masiva.js",
    "module": "./dist/sat-ws-descarga-masiva.modern.mjs",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prepare": "is-ci || husky install",
        "lint": "eslint --fix --ext .js,.ts .",
        "lint:check": "eslint --ext .js,.ts .",
        "format": "prettier --write \"**/*.{js,ts,json}\"",
        "format:check": "prettier --check \"**/*.{js,ts,json}\"",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "build": "pnpm prebuild && tsup && pnpm gen:types",
        "prebuild": "rimraf dist",
        "gen:docs": "rimraf docs && typedoc --options typedoc.json",
        "gen:types": "api-extractor run --local",
        "release": "pnpm run build && pnpm changeset publish"
    },
    "repository": "https://github.com/nodecfdi/sat-ws-descarga-masiva",
    "bugs": {
        "url": "https://github.com/nodecfdi/sat-ws-descarga-masiva/issues"
    },
    "homepage": "https://github.com/nodecfdi/sat-ws-descarga-masiva",
    "devDependencies": {
        "@changesets/cli": "^2.26.1",
        "@commitlint/cli": "^17.5.1",
        "@microsoft/api-extractor": "^7.34.4",
        "@nrsk/config-conventional": "^1.0.0",
        "@peculiar/webcrypto": "^1.4.3",
        "@types/is-ci": "^3.0.0",
        "@types/jsrsasign": "^10.5.8",
        "@types/luxon": "^3.3.0",
        "@types/node": "^18.15.11",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "@vitest/coverage-istanbul": "^0.30.1",
        "@xmldom/xmldom": "^0.8.7",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-xo": "^0.43.1",
        "eslint-config-xo-typescript": "^0.57.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "eslint-plugin-unicorn": "^46.0.0",
        "husky": "^8.0.3",
        "is-ci": "^3.0.1",
        "jest-xml-matcher": "^1.2.0",
        "lint-staged": "^13.2.1",
        "prettier": "^2.8.7",
        "rimraf": "^5.0.0",
        "tsup": "^6.7.0",
        "typedoc": "^0.23.28",
        "typedoc-theme-hierarchy": "^3.1.0",
        "typescript": "^5.0.4",
        "vite": "^4.2.1",
        "vitest": "^0.30.1",
        "xadesjs": "^2.4.4",
        "xpath": "^0.0.32"
    },
    "dependencies": {
        "@nodecfdi/cfdiutils-common": "^1.2.4",
        "@nodecfdi/credentials": "^1.3.0",
        "@nodecfdi/rfc": "^1.3.1",
        "jszip": "^3.10.1",
        "luxon": "^3.3.0"
    },
    "engines": {
        "node": ">= 14.18.0"
    }
