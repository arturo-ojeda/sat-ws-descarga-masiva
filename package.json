{
    "name": "@nodecfdi/sat-ws-descarga-masiva",
    "version": "1.0.0",
    "description": "Librer√≠a para usar el servicio web del SAT de Descarga Masiva",
    "keywords": [
        "sat",
        "cfdi",
        "download",
        "descarga",
        "webservice"
    ],
    "private": false,
    "author": "OcelotlStudio <contacto@ocelotlstudio.com>",
    "license": "MIT",
    "type": "module",
    "main": "./dist/prod/sat-ws-descarga-masiva.cjs",
    "module": "./dist/prod/sat-ws-descarga-masiva.modern.js",
    "umd:main": "./dist/prod/sat-ws-descarga-masiva.umd.js",
    "exports": {
        "require": "./dist/prod/sat-ws-descarga-masiva.cjs",
        "development": "./dist/dev/sat-ws-descarga-masiva.modern.js",
        "default": "./dist/prod/sat-ws-descarga-masiva.modern.js"
    },
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "lint": "eslint --fix \"src/**/\"",
        "lint:check": "eslint \"src/**/\"",
        "lint:ci": "pnpm lint:check",
        "test": "jest --verbose --coverage",
        "test:watch": "jest --watch",
        "test:ci": "jest --runInBand --ci",
        "test:ci:coverage": "jest --runInBand --ci --coverage --coverageDirectory=coverage",
        "build:prod": "cross-env NODE_ENV=production microbundle --tsconfig ./tsconfig.jso --target node  --format modern,cjs  --output dist/prod/sat-ws-descarga-masiva.js",
        "build:dev": "cross-env NODE_ENV=development microbundle --define process.env.NODE_ENV=development --tsconfig ./tsconfig.json --target node --generateTypes false --format modern --output dist/dev/sat-ws-descarga-masiva.js",
        "build": "rm -rf ./dist && pnpm build:dev && pnpm build:prod",
        "gen:docs": "rm -rf ./docs && typedoc --options typedoc.cjs",
        "prepublishOnly": "pnpm build",
        "prepare": "husky install",
        "release": "pnpm run prepublishOnly && pnpm changeset publish"
    },
    "repository": "https://github.com/nodecfdi/sat-ws-descarga-masiva",
    "bugs": {
        "url": "https://github.com/nodecfdi/sat-ws-descarga-masiva/issues"
    },
    "homepage": "https://github.com/nodecfdi/sat-ws-descarga-masiva",
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@changesets/cli": "^2.25.2",
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@peculiar/webcrypto": "^1.4.1",
        "@types/jest": "^29.2.3",
        "@types/luxon": "^3.1.0",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.44.0",
        "@typescript-eslint/parser": "^5.44.0",
        "@xmldom/xmldom": "^0.8.6",
        "babel-plugin-dev-expression": "^0.2.3",
        "babel-plugin-transform-define": "^2.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jest": "^27.1.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-tsdoc": "^0.2.17",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "jest-mock-console": "^2.0.0",
        "jest-mock-extended": "^3.0.1",
        "jest-watch-typeahead": "^2.2.1",
        "lint-staged": "^13.0.3",
        "microbundle": "^0.15.1",
        "prettier": "^2.8.0",
        "typedoc": "^0.23.21",
        "typedoc-theme-hierarchy": "^3.0.0",
        "typescript": "^4.9.3",
        "xadesjs": "^2.4.4",
        "xpath": "^0.0.32"
    },
    "dependencies": {
        "@nodecfdi/cfdiutils-common": "^1.2.1",
        "@nodecfdi/credentials": "^1.3.0",
        "@nodecfdi/rfc": "^1.2.2",
        "jszip": "^3.10.1",
        "luxon": "^3.1.0"
    },
    "engines": {
        "node": ">= 14.18.0"
    }
}
